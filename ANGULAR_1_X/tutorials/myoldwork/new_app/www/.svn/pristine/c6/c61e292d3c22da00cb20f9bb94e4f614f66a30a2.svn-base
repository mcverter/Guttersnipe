    <script src="bower_components/novafoundation/app/scripts/services/user.js"></script>
    <script src="bower_components/novafoundation/app/scripts/services/category.js"></script>
    <script src="bower_components/novafoundation/app/scripts/services/nav.js"></script>
    <script src="bower_components/novafoundation/app/scripts/services/report.js"></script>
