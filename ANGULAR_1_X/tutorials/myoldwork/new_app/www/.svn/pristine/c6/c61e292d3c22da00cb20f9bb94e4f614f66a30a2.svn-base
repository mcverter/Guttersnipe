    <script src="bower_components/novafoundation/app/scripts/controllers/admin/users.js"></script>
    <script src="bower_components/novafoundation/app/scripts/controllers/admin/deploy.js"></script>
    <script src="bower_components/novafoundation/app/scripts/controllers/admin/metadata.js"></script>


    <script src="bower_components/novafoundation/app/scripts/controllers/widgets/hierarchical_selector.js"></script>
