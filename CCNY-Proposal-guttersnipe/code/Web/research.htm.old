<html>
<head>
<title>Find vacant properties </title>

<script language="javascript">
function checkSubmit() {
var states = document.frm.state;

if (!states.options[states.selectedIndex]) {
alert ("Please enter some information for the search\nYou must enter at least a value for the state\n");
document.frm.state.focus();
return false;
}
return true;
}
</script>
